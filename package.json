  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "start": "node dist/server/index.mjs",
    "build": "rimraf dist && pkgroll"
  },
  "dependencies": {
    "@circle-fin/developer-controlled-wallets": "^4.5.1",
    "@hono/node-server": "^1.13.1",
    "@hono/swagger-ui": "^0.4.1",
    "@hono/trpc-server": "^0.3.2",
    "@hono/zod-validator": "^0.4.1",
    "@trpc/server": "^10.45.2",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.5",
    "hono": "^4.6.3",
    "trpc-openapi": "^1.2.0",
    "trpc-panel": "^1.3.4",
    "xstate": "^5.18.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "esbuild": "^0.24.0",
    "pkgroll": "^2.5.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.6.2"
  }
